<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="/1684828896.ico">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
    <header>
    {% include 'Include/_navbar.html.twig' %}
    </header>
     {% for season, messages in app.flashes %}
        {% for message in messages %}
        <div class="alert alert-{{ season }}">
            {{ message }}
        </div>
    {% endfor %}
     {% endfor %}

      {% for epsidode, messages in app.flashes %}
        {% for message in messages %}
        <div class="alert alert-{{ season }}">
            {{ message }}
        </div>
    {% endfor %}
    
     {% endfor %}
        {% block body %}{% endblock %}
    </body>
</html>
